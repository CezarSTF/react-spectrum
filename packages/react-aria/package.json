{
  "name": "react-aria",
  "version": "3.33.1",
  "description": "Spectrum UI components in React",
  "license": "Apache-2.0",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/import.mjs",
      "require": "./dist/main.js"
    },
    "./i18n": {
      "types": "./i18n/index.d.ts",
      "import": "./i18n/index.mjs",
      "require": "./i18n/index.js"
    },
    "./i18n/*": {
      "types": "./i18n/lang.d.ts",
      "import": "./i18n/*.mjs",
      "require": "./i18n/*.js"
    }
  },
  "types": "dist/types.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist",
    "i18n"
  ],
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "mkdir -p dist; cp src/index.ts dist/types.d.ts; grep -v '^export type' src/index.ts > dist/module.js; babel --root-mode upward src/index.ts -o dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/react-spectrum"
  },
  "dependencies": {
    "@internationalized/string": "workspace:^",
    "@react-aria/breadcrumbs": "workspace:^",
    "@react-aria/button": "workspace:^",
    "@react-aria/calendar": "workspace:^",
    "@react-aria/checkbox": "workspace:^",
    "@react-aria/combobox": "workspace:^",
    "@react-aria/datepicker": "workspace:^",
    "@react-aria/dialog": "workspace:^",
    "@react-aria/dnd": "workspace:^",
    "@react-aria/focus": "workspace:^",
    "@react-aria/gridlist": "workspace:^",
    "@react-aria/i18n": "workspace:^",
    "@react-aria/interactions": "workspace:^",
    "@react-aria/label": "workspace:^",
    "@react-aria/link": "workspace:^",
    "@react-aria/listbox": "workspace:^",
    "@react-aria/menu": "workspace:^",
    "@react-aria/meter": "workspace:^",
    "@react-aria/numberfield": "workspace:^",
    "@react-aria/overlays": "workspace:^",
    "@react-aria/progress": "workspace:^",
    "@react-aria/radio": "workspace:^",
    "@react-aria/searchfield": "workspace:^",
    "@react-aria/select": "workspace:^",
    "@react-aria/selection": "workspace:^",
    "@react-aria/separator": "workspace:^",
    "@react-aria/slider": "workspace:^",
    "@react-aria/ssr": "workspace:^",
    "@react-aria/switch": "workspace:^",
    "@react-aria/table": "workspace:^",
    "@react-aria/tabs": "workspace:^",
    "@react-aria/tag": "workspace:^",
    "@react-aria/textfield": "workspace:^",
    "@react-aria/tooltip": "workspace:^",
    "@react-aria/utils": "workspace:^",
    "@react-aria/visually-hidden": "workspace:^",
    "@react-types/shared": "workspace:^"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0-rc.1 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0-rc.1 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
