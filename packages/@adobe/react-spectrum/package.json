    "@react-types/shared": "workspace:^",
    "client-only": "^0.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0-rc.1 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0-rc.1 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10"
  }
