{
  "name": "@adobe/react-spectrum",
  "version": "3.35.0",
  "description": "Spectrum UI components in React",
  "license": "Apache-2.0",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/import.mjs",
      "require": "./dist/main.js"
    },
    "./i18n": {
      "types": "./i18n/index.d.ts",
      "import": "./i18n/index.mjs",
      "require": "./i18n/index.js"
    },
    "./i18n/*": {
      "types": "./i18n/lang.d.ts",
      "import": "./i18n/*.mjs",
      "require": "./i18n/*.js"
    }
  },
  "types": "dist/types.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist",
    "i18n"
  ],
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "mkdir -p dist; cp src/index.ts dist/types.d.ts; grep -v '^export type' src/index.ts > dist/module.js; babel --root-mode upward src/index.ts -o dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/react-spectrum"
  },
  "dependencies": {
    "@internationalized/string": "workspace:^",
    "@react-aria/i18n": "workspace:^",
    "@react-aria/ssr": "workspace:^",
    "@react-aria/utils": "workspace:^",
    "@react-aria/visually-hidden": "workspace:^",
    "@react-spectrum/actionbar": "workspace:^",
    "@react-spectrum/actiongroup": "workspace:^",
    "@react-spectrum/avatar": "workspace:^",
    "@react-spectrum/badge": "workspace:^",
    "@react-spectrum/breadcrumbs": "workspace:^",
    "@react-spectrum/button": "workspace:^",
    "@react-spectrum/buttongroup": "workspace:^",
    "@react-spectrum/calendar": "workspace:^",
    "@react-spectrum/checkbox": "workspace:^",
    "@react-spectrum/combobox": "workspace:^",
    "@react-spectrum/contextualhelp": "workspace:^",
    "@react-spectrum/datepicker": "workspace:^",
    "@react-spectrum/dialog": "workspace:^",
    "@react-spectrum/divider": "workspace:^",
    "@react-spectrum/dnd": "workspace:^",
    "@react-spectrum/dropzone": "workspace:^",
    "@react-spectrum/filetrigger": "workspace:^",
    "@react-spectrum/form": "workspace:^",
    "@react-spectrum/icon": "workspace:^",
    "@react-spectrum/illustratedmessage": "workspace:^",
    "@react-spectrum/image": "workspace:^",
    "@react-spectrum/inlinealert": "workspace:^",
    "@react-spectrum/labeledvalue": "workspace:^",
    "@react-spectrum/layout": "workspace:^",
    "@react-spectrum/link": "workspace:^",
    "@react-spectrum/list": "workspace:^",
    "@react-spectrum/listbox": "workspace:^",
    "@react-spectrum/menu": "workspace:^",
    "@react-spectrum/meter": "workspace:^",
    "@react-spectrum/numberfield": "workspace:^",
    "@react-spectrum/overlays": "workspace:^",
    "@react-spectrum/picker": "workspace:^",
    "@react-spectrum/progress": "workspace:^",
    "@react-spectrum/provider": "workspace:^",
    "@react-spectrum/radio": "workspace:^",
    "@react-spectrum/searchfield": "workspace:^",
    "@react-spectrum/slider": "workspace:^",
    "@react-spectrum/statuslight": "workspace:^",
    "@react-spectrum/switch": "workspace:^",
    "@react-spectrum/table": "workspace:^",
    "@react-spectrum/tabs": "workspace:^",
    "@react-spectrum/tag": "workspace:^",
    "@react-spectrum/text": "workspace:^",
    "@react-spectrum/textfield": "workspace:^",
    "@react-spectrum/theme-dark": "workspace:^",
    "@react-spectrum/theme-default": "workspace:^",
    "@react-spectrum/theme-light": "workspace:^",
    "@react-spectrum/tooltip": "workspace:^",
    "@react-spectrum/view": "workspace:^",
    "@react-spectrum/well": "workspace:^",
    "@react-stately/collections": "workspace:^",
