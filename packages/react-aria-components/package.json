{
  "name": "react-aria-components",
  "version": "1.1.1",
  "description": "A library of styleable components built using React Aria",
  "license": "Apache-2.0",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/import.mjs",
      "require": "./dist/main.js"
    },
    "./i18n": {
      "types": "./i18n/index.d.ts",
      "import": "./i18n/index.mjs",
      "require": "./i18n/index.js"
    },
    "./i18n/*": {
      "types": "./i18n/lang.d.ts",
      "import": "./i18n/*.mjs",
      "require": "./i18n/*.js"
    }
  },
  "files": [
    "dist",
    "src",
    "i18n"
  ],
  "sideEffects": [
    "*.css"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/react-spectrum"
  },
  "dependencies": {
    "@internationalized/date": "workspace:^",
    "@internationalized/string": "workspace:^",
    "@react-aria/focus": "workspace:^",
    "@react-aria/interactions": "workspace:^",
    "@react-aria/menu": "workspace:^",
    "@react-aria/toolbar": "workspace:^",
    "@react-aria/tree": "workspace:^",
    "@react-aria/utils": "workspace:^",
    "@react-stately/menu": "workspace:^",
    "@react-stately/table": "workspace:^",
    "@react-stately/utils": "workspace:^",
    "@react-types/form": "workspace:^",
    "@react-types/grid": "workspace:^",
    "@react-types/shared": "workspace:^",
    "@react-types/table": "workspace:^",
    "@swc/helpers": "^0.5.0",
    "client-only": "^0.0.1",
    "react-aria": "workspace:^",
    "react-stately": "workspace:^",
    "use-sync-external-store": "^1.2.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0-rc.1 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0-rc.1 || ^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
